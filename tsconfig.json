{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "skipLibCheck": true,
      "allowJs": true,
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "module": "Commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react-jsx",
      "noEmit": true,
      "types": ["web-vitals", "node"]
    },
    "include": ["src", "path/to/your/svg.d.ts"]
  }
  